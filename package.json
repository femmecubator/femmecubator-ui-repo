{
  "name": "femmecubator-ui-repo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.3.0",
    "@hookform/resolvers": "1.3.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.0",
    "focus-trap-react": "^8.5.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "msw": "^0.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.2.0",
    "react-hook-form": "^6.15.5",
    "react-horizontal-scrolling-menu": "^0.7.10",
    "react-material-ui-carousel": "^2.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "select-timezone-material-ui": "^2.6.0",
    "styled-components": "^5.2.3",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local:backend": "cd ../femmecubator-api-repo && npm run start-debug",
    "start:local:frontend": "cd ../femmecubator-ui-repo && npm start",
    "start-local": "run-p start:local:* -- --watch",
    "start:mock": "cross-env REACT_APP_MOCK_API_TRUE=true npm start",
    "build": "react-scripts build",
    "storybook": "start-storybook -p 6006 -s public --no-dll",
    "build-storybook": "build-storybook -s public --no-dll",
    "test": "react-scripts test",
    "test:ghook": "cross-env CI=true react-scripts test --watch=all",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "eslint -f checkstyle . -o output/eslint/lint-results.xml && eslint -f html . -o output/eslint/eslint-report.html",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "fix": "run-p fix:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@axe-core/react": "^4.1.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/preset-react": "^7.13.13",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@storybook/addon-controls": "^6.2.5",
    "@storybook/addon-essentials": "^6.2.5",
    "@storybook/addon-links": "^6.2.5",
    "@storybook/node-logger": "^6.2.5",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.5",
    "@testing-library/dom": "^7.30.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^13.1.8",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "dot-env": "0.0.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ghooks": "^2.0.4",
    "lint-staged": "^10.5.2",
    "miragejs": "^0.1.41",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:ghook"
    }
  }
}
